<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.buet.DemographicsController">
    <children>
      <HBox prefHeight="494.0" prefWidth="1240.0" spacing="20.0">
         <children>
            <TableView fx:id="countryTable" prefHeight="299.0" prefWidth="1240.0">
              <columns>
                <TableColumn fx:id="countryColumn" prefWidth="290.0" text="Country" />
                <TableColumn fx:id="countryNumColumn" prefWidth="296.0" text="No. of players" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <TableView layoutX="10.0" layoutY="10.0" prefHeight="299.0" prefWidth="1240.0">
               <columns>
                  <TableColumn prefWidth="290.0" text="Country" />
                  <TableColumn prefWidth="296.0" text="No. of players" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
        <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="Back to Search" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
